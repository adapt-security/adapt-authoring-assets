{
  "name": "adapt-authoring-assets",
  "version": "1.3.2",
  "description": "Assets API for Adapt Authoring Tool",
  "homepage": "https://github.com/adapt-security/adapt-authoring-assets",
  "license": "GPL-3.0",
  "type": "module",
  "main": "index.js",
  "repository": "github:adapt-security/adapt-authoring-assets",
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@ffprobe-installer/ffprobe": "^2.1.2",
    "mime": "^4.1.0"
  },
  "peerDependencies": {
    "adapt-authoring-authored": "^1.1.1",
    "adapt-authoring-core": "^1.7.0",
    "adapt-authoring-middleware": "^1.0.2",
    "adapt-authoring-tags": "^1.0.2"
  },
  "peerDependenciesMeta": {
    "adapt-authoring-authored": {
      "optional": true
    },
    "adapt-authoring-core": {
      "optional": true
    },
    "adapt-authoring-middleware": {
      "optional": true
    },
    "adapt-authoring-tags": {
      "optional": true
    }
  },
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "conventional-changelog-eslint": "^6.0.0",
    "semantic-release": "^25.0.2",
    "standard": "^17.1.0"
  },
  "release": {
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "eslint"
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "eslint"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json"
          ],
          "message": "Chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
